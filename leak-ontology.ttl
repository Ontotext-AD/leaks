leak:sourceID a owl:DatatypeProperty;
  rdfs:isDefinedBy leak:;
  rdfs:label "sourceID";
  rdfs:domain leak:Node;
  rdfs:comment """ "Panama Papers" or "Offshore Leaks" """.

