  rdfs:comment "ICIJ identifier".

leak:internal_id a owl:DatatypeProperty;
  rdfs:isDefinedBy leak:;
  rdfs:label "internal_id";
  rdfs:domain leak:Node;
  rdfs:comment "???".

leak:name a owl:DatatypeProperty;
